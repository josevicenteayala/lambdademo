{
  "lambda-basic-execution": {
    "policy_content": {
      "Statement": [
        {
          "StatementId": "FunctionURLAllowPublicAccess",
          "Action": [
            "logs:CreateLogGroup",
            "logs:CreateLogStream",
            "logs:PutLogEvents",
            "lambda:InvokeFunctionUrl"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:lambda:eu-central-1:196241772369:function:cmtr-b9f1a140-hello_world",
          "Condition": {
            "StringEquals": {
              "lambda:FunctionUrlAuthType": "NONE"
            }
          }
        }
      ],
      "Version": "2012-10-17"
    },
    "resource_type": "iam_policy"
  },
  "hello_world-role": {
    "predefined_policies": [],
    "principal_service": "lambda",
    "custom_policies": [
      "lambda-basic-execution"
    ],
    "resource_type": "iam_role"
  }
}